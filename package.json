    "@babel/core": "^7.17.9",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "cypress": "^9.5.3",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.4.12",
    "react-docgen-typescript": "^2.2.2",
