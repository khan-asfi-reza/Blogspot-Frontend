{
  "name": "firebolt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "cypress": "cypress open",
    "dev:up": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "prod:up": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docs": "styleguidist server --config styleguide.config.js",
    "tailwind:dist": "npx tailwindcss -w -c ./docs/tailwinddocs.config.js -i src/styles/globals.css -o docs/dist/dist.css --minify",
    "docs:start": "nodemon --exec \"npm run docs\""
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@headlessui/react": "^1.5.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.4",
    "axios": "^0.26.1",
    "babel-jest": "^27.5.1",
    "classnames": "^2.3.1",
    "cssnano": "^5.1.7",
    "enzyme": "^3.11.0",
    "file-loader": "^6.2.0",
    "framer-motion": "^6.2.9",
    "jest": "^27.5.1",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-blaze-carousel": "^0.1.3",
    "react-docgen": "^5.4.0",
    "react-dom": "18.0.0",
    "react-icon": "^1.0.0",
